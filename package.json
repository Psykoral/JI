{
	"name": "grunt-starter-kit",
	"version": "1.0.1",
	"description": "FlippyUI Grunt stack starter kit",
	"main": "src/js/main.js",
	"scripts": {
		"test": "grunt test",
		"livetests": "grunt livetests",
		"compile": "grunt",
		"web": "grunt live"
	},
	"jscsConfig": {
		"preset": "crockford",
		"disallowDanglingUnderscores": {
			"allExcept": [
				"_super"
			]
		},
		"validateIndentation": "\t",
		"requireDotNotation": {
			"allExcept": [
				"snake_case"
			]
		}
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Psykoral/grunt-starter-kit.git"
	},
	"keywords": [
		"grunt",
		"node",
		"starter kit",
		"lessss",
		"canjs",
		"font awesome"
	],
	"author": "Flippydisk",
	"contributors": [
		{
			"name": "Psykoral",
			"email": "psykoral@flippydisk.com"
		}
	],
	"homepage": "http://flippydisk.com",
	"license": "SEE LICENSE IN LICENSE.txt",
	"dependencies": {
		"grunt-template-jasmine-requirejs": "https://github.com/radum/grunt-template-jasmine-requirejs.git",
		"phantomjs-prebuilt": "~2.1.7",
		"lodash": "^4.13.1"
	},
	"devDependencies": {
		"bower": "^1.7.9",
		"grunt": "~1.0.1",
		"grunt-assemble": "~0.4.0",
		"grunt-autoprefixer": "^3.0.3",
		"grunt-bump": "0.8.0",
		"grunt-npm": "0.0.2",
		"grunt-cli": "~1.2.0",
		"grunt-concurrent": "~1.0.0",
		"grunt-contrib-clean": "~1.0.0",
		"grunt-contrib-concat": "~1.0.1",
		"grunt-contrib-connect": "~1.0.2",
		"grunt-contrib-copy": "~1.0.0",
		"grunt-contrib-jasmine": "~1.0.0",
		"grunt-contrib-jshint": "~1.0.0",
		"grunt-contrib-less": "~1.3.0",
		"grunt-contrib-requirejs": "~1.0.0",
		"grunt-contrib-watch": "~1.0.0",
		"grunt-jscs": "^3.0.0",
		"grunt-jsdoc": "~2.1.0",
		"grunt-string-replace": "^1.3.0-beta.1",
		"less-plugin-autoprefix": "^1.5.1",
		"less-plugin-clean-css": "^1.5.0",
		"load-grunt-tasks": "~3.5.0",
		"time-grunt": "~1.3.0"
	},
	"engines": {
		"node": ">=4"
	}
}
